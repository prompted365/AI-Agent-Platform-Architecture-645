# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/swarm_agents?schema=public"

# Server Configuration
PORT=3001
NODE_ENV=development

# Event Bus Configuration (Optional)
REDIS_URL=redis://localhost:6379

# LLM Provider (Requesty.ai) - REQUIRED
REQUESTY_API_KEY=your_requesty_api_key_here

# GitHub Integration (Optional)
GITHUB_TOKEN=your_github_token_here

# Supabase Configuration (Optional - set ENABLE_SUPABASE=false to disable)
ENABLE_SUPABASE=false
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# Astra DB Configuration (Optional - only used if Supabase is disabled)
ASTRA_DB_ID=your_astra_db_id_here
ASTRA_DB_REGION=your_astra_db_region_here
ASTRA_DB_TOKEN=your_astra_db_token_here

# Railway Deployment (Optional)
RAILWAY_TOKEN=your_railway_token_here

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100